    "name": "Open Sacramento",
    "email": "hello@opensac.org",
    "url": "https://opensac.org"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/code4sac/opensac.org/issues"
  },
  "homepage": "https://github.com/code4sac/opensac.org#readme",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "jest": "^29.7.0",
    "js-yaml": "^4.1.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.69.3"
  },
  "dependencies": {
    "bootstrap": "^5.3.2"
  }
}
